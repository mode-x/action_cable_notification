{
  "editor.tabSize": 2,
  "vscode-erb-beautify.bundlerPath": "/Users/nase/.rbenv/shims/bundler",
  "vscode-erb-beautify.executePath": "/Users/nase/.rbenv/shims/htmlbeautifier",
  "[erb]": {
    "editor.defaultFormatter": "aliariff.vscode-erb-beautify",
    "editor.formatOnSave": true
  },
  "files.associations": {
    "*.html.erb": "erb"
  },
  "[html]": {
    "editor.defaultFormatter": "aliariff.vscode-erb-beautify",
  },
  "emmet.includeLanguages": {
    "erb": "html"
  },
  "vscode-erb-beautify.customEnvVar": {
    "LC_ALL": "en_US.UTF-8"
  },
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showSuggestionsAsSnippets": true
}